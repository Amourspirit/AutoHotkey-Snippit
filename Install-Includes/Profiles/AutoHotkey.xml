<?xml version="1.0"?>
<profile xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="0.1.0.0" minVersion="0.1.0" name="AutoHotkey" schemaVersion="1.1">
  <codeLanguage>
    <codeName>AutoHotkey</codeName>
    <description>AutoHotKey Programming Language Automation Plugins</description>
    <paths>
      <mainData>AutoHotkey</mainData>
      <plugin>autohotkey</plugin>
      <snips>autohotkey</snips>
    </paths>
  </codeLanguage>
  <windows>
    <window>
      <name>notepad++</name>
      <value>ahk_class Notepad++</value>
    </window>
    <window>
      <name>Scite</name>
      <value>ahk_class SciTEWindow</value>
    </window>
    <window>
      <name>SublimeText</name>
      <value>ahk_class PX_WINDOW_CLASS</value>
    </window>
    <window>
      <name>AHK Studio</name>
      <value>ahk_exe AHK-Studio.exe</value>
    </window>
  </windows>
  <profileEndChars>
    <profileEndChar>Tab</profileEndChar>
    <profileEndChar>Enter</profileEndChar>
  </profileEndChars>
</profile>
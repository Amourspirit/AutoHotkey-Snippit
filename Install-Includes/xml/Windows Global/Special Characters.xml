      <hotstrings>
        <hotstring trigger="]RTM">
          <name>® Registered Trade Mark</name>
          <description>Wirtes ® Registered Trademak</description>
          <category>Char</category>
        </hotstring>
        <hotstring trigger="]CCR">
          <name>© Copyright</name>
          <description>Writes © Copyright</description>
          <category>Char</category>
        </hotstring>
      </hotstrings>
      <commands />
    </include>

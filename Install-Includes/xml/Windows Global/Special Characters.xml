  <commands />
  <hotstrings />
  <includes>
    <include>
      <name>Special Chars</name>
      <description>Special Chars such as © and ®</description>
      <snippit>hotstring_special_chars.ahk</snippit>
      <code>;{  Hotstrings Special Chars
:o?:]rtm:: ; Registered trade mark ΍
